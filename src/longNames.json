      "customerDataTableSchema": {
        "tableName": "customers_v2",
        "tableDescription": "Schema definition for the customer data table in the primary database.",
        "columnDefinitions": [
          {
            "columnName": "customerUniqueIdentifierId",
            "dataType": "UUID",
            "isPrimaryKey": true,
            "isNullable": false,
            "columnDescription": "Unique identifier for each customer record."
          },
          {
            "columnName": "customerFirstName",
            "dataType": "VARCHAR(255)",
            "isPrimaryKey": false,
            "isNullable": false,
            "columnDescription": "First name of the customer."
          },
          {
            "columnName": "customerLastName",
            "dataType": "VARCHAR(255)",
            "isPrimaryKey": false,
            "isNullable": false,
            "columnDescription": "Last name of the customer."
          },
          {
            "columnName": "customerEmailAddress",
            "dataType": "VARCHAR(255)",
            "isPrimaryKey": false,
            "isNullable": true,
            "columnDescription": "Email address of the customer, may be null."
          },
          {
            "columnName": "customerRegistrationTimestamp",
            "dataType": "TIMESTAMP WITH TIME ZONE",
            "isPrimaryKey": false,
            "isNullable": false,
            "columnDescription": "Timestamp when the customer registered."
          }
        ],
        "tableIndices": [
          {
            "indexName": "idx_customer_email",
            "indexedColumns": ["customerEmailAddress"],
            "indexType": "BTREE",
            "isUniqueIndex": true,
            "indexDescription": "Index on customer email for fast lookups."
          }
        ]
      },
